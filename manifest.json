{
  "manifest_version": 3,
  "name": "Block DevTools",
  "version": "1.0",
  "description": "Extension pour bloquer l'ouverture des DevTools.",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
