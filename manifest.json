{
  "manifest_version": 3,
  "name": "EscapeGame - Bapt-tech",
  "version": "1.1",
  "description": "Use with Escape Game (https://bapt-tech.github.io/EscapeGame)",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
